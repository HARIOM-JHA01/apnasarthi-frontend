{
  "extends": "next/core-web-vitals",
  "rules" : {
    "no-console": "warn",
    "no-debugger": "warn",
    "no-alert": "warn",
    "no-unused-vars": "warn",
    "no-undef": "warn",
    "no-constant-condition": "warn",
    "no-unreachable": "warn",
